{
  "name": "yihan-study-room",
  "version": "1.0.0",
  "description": "意涵自习室 - uni-app跨端应用",
  "main": "main.js",
  "scripts": {
    "dev:app": "uni build --watch",
    "dev:app-android": "uni build --watch --platform app-android",
    "dev:app-ios": "uni build --watch --platform app-ios",
    "dev:h5": "uni serve",
    "dev:mp-alipay": "uni build --watch --platform mp-alipay",
    "dev:mp-baidu": "uni build --watch --platform mp-baidu",
    "dev:mp-kuaishou": "uni build --watch --platform mp-kuaishou",
    "dev:mp-lark": "uni build --watch --platform mp-lark",
    "dev:mp-qq": "uni build --watch --platform mp-qq",
    "dev:mp-toutiao": "uni build --watch --platform mp-toutiao",
    "dev:mp-weixin": "uni build --watch --platform mp-weixin",
    "dev:quickapp-webview": "uni build --watch --platform quickapp-webview",
    "build:app": "uni build",
    "build:app-android": "uni build --platform app-android",
    "build:app-ios": "uni build --platform app-ios",
    "build:h5": "uni build --platform h5",
    "build:mp-alipay": "uni build --platform mp-alipay",
    "build:mp-baidu": "uni build --platform mp-baidu",
    "build:mp-kuaishou": "uni build --platform mp-kuaishou",
    "build:mp-lark": "uni build --platform mp-lark",
    "build:mp-qq": "uni build --platform mp-qq",
    "build:mp-toutiao": "uni build --platform mp-toutiao",
    "build:mp-weixin": "uni build --platform mp-weixin",
    "build:quickapp-webview": "uni build --platform quickapp-webview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@dcloudio/uni-app": "^3.0.0",
    "@dcloudio/uni-app-plus": "^3.0.0",
    "@dcloudio/uni-components": "^3.0.0",
    "@dcloudio/uni-h5": "^3.0.0",
    "@dcloudio/uni-mp-alipay": "^3.0.0",
    "@dcloudio/uni-mp-baidu": "^3.0.0",
    "@dcloudio/uni-mp-kuaishou": "^3.0.0",
    "@dcloudio/uni-mp-lark": "^3.0.0",
    "@dcloudio/uni-mp-qq": "^3.0.0",
    "@dcloudio/uni-mp-toutiao": "^3.0.0",
    "@dcloudio/uni-mp-weixin": "^3.0.0",
    "@dcloudio/uni-quickapp-webview": "^3.0.0",
    "vue": "^3.4.21",
    "pinia": "^2.0.36",
    "@uni-helper/uni-use": "^0.2.1",
    "dayjs": "^1.11.10",
    "luch-request": "^3.1.1",
    "uni-simple-router": "^2.0.7",
    "@dcloudio/uni-ui": "^1.5.5"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.8",
    "@dcloudio/uni-automator": "^3.0.0",
    "@dcloudio/uni-cli-shared": "^3.0.0",
    "@dcloudio/vite-plugin-uni": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vue-tsc": "^2.0.16"
  },
  "uni-app": {
    "scripts": {}
  },
  "browserslist": [
    "Android >= 4.4",
    "ios >= 9"
  ],
  "keywords": [
    "uni-app",
    "vue3",
    "typescript",
    "自习室",
    "预订",
    "跨端应用"
  ],
  "author": "yihan-team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anhengchangyua/zishi.git"
  }
}